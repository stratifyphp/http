{
    "name": "stratify/http",
    "description": "HTTP middlewares for PSR-7 and PSR-15",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Stratify\\Http\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stratify\\Http\\Test\\": "tests"
        }
    },
    "require": {
        "php": ">=7.1",
        "zendframework/zend-diactoros": "^1.3.5",
        "http-interop/http-middleware": "~0.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    }
}
