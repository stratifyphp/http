{
    "name": "stratify/http",
    "description": "HTTP middlewares for PSR-7 and PSR-15",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Stratify\\Http\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stratify\\Http\\Test\\": "tests"
        }
    },
    "require": {
        "php": ">=7.4",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "laminas/laminas-diactoros": "^2.0",
        "laminas/laminas-httphandlerrunner": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
